<?xml version='1.0' encoding='UTF-8'?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
  <description>Example permission set</description>
  <hasActivationRequired>true</hasActivationRequired>
  <label>Example_Permission_Set</label>
  <license>Salesforce</license>
  <userLicense>Salesforce</userLicense>
  <agentAccesses>
    <agentName>SupportAgent</agentName>
    <enabled>true</enabled>
  </agentAccesses>
  <applicationVisibilities>
    <application>MyApp</application>
    <visible>true</visible>
  </applicationVisibilities>
  <classAccesses>
    <apexClass>MyClass</apexClass>
    <enabled>true</enabled>
  </classAccesses>
  <customMetadataTypeAccesses>
    <name>CustomType__mdt</name>
    <enabled>true</enabled>
  </customMetadataTypeAccesses>
  <customPermissions>
    <name>MyCustomPermission</name>
    <enabled>true</enabled>
  </customPermissions>
  <customSettingAccesses>
    <name>CustomSetting__c</name>
    <enabled>true</enabled>
  </customSettingAccesses>
  <emailRoutingAddressAccesses>
    <emailRoutingAddress>support@example.com</emailRoutingAddress>
    <enabled>true</enabled>
  </emailRoutingAddressAccesses>
  <externalCredentialPrincipalAccesses>
    <externalCredentialPrincipal>Credential1</externalCredentialPrincipal>
    <enabled>true</enabled>
  </externalCredentialPrincipalAccesses>
  <externalDataSourceAccesses>
    <externalDataSource>MyDataSource</externalDataSource>
    <enabled>true</enabled>
  </externalDataSourceAccesses>
  <fieldPermissions>
    <field>Account.Name</field>
    <editable>true</editable>
    <readable>true</readable>
  </fieldPermissions>
  <flowAccesses>
    <flow>MyFlow</flow>
    <enabled>true</enabled>
  </flowAccesses>
  <objectPermissions>
    <object>Account</object>
    <allowCreate>true</allowCreate>
    <allowRead>true</allowRead>
    <allowEdit>true</allowEdit>
    <allowDelete>false</allowDelete>
    <viewAllRecords>false</viewAllRecords>
    <modifyAllRecords>false</modifyAllRecords>
  </objectPermissions>
  <pageAccesses>
    <apexPage>MyPage</apexPage>
    <enabled>true</enabled>
  </pageAccesses>
  <recordTypeVisibilities>
    <recordType>Account.MyRecordType</recordType>
    <visible>true</visible>
    <default>false</default>
  </recordTypeVisibilities>
  <ServicePresenceStatusAccesses>
    <servicePresenceStatus>Available</servicePresenceStatus>
    <enabled>true</enabled>
  </ServicePresenceStatusAccesses>
  <tabSettings>
    <tab>Account</tab>
    <visibility>Available</visibility>
  </tabSettings>
  <userPermissions>
    <name>APIEnabled</name>
    <enabled>true</enabled>
  </userPermissions>
</PermissionSet>
